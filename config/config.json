{
    "server": {
        "accessLog": "/var/log/hipache/hipache.log",
        "errorLog": "/var/log/hipache/hipache.err",
        "workers": 10,
        "maxSockets": 100,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "retryOnError": 3,
        "deadBackendOn500": true,
        "httpKeepAlive": false,
        "staticDir": null
    },
    "http": {
        "port": 80,
        "bind": [ "0.0.0.0" ]
    },
    "driver": "redis://redis"
}
